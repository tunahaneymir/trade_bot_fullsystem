1.2 Experience Buffer
EXPERIENCE_BUFFER = {
    'capacity': 100000,  # Last 100K experiences
    'structure': {
        'trade_id': 'unique_identifier',
        'timestamp': '2024-01-15 10:30:00',
        
        # Market data
        'coin': 'BTCUSDT',
        'timeframe': '15m',
        

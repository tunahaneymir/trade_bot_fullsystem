            'trend_strength': 0.8,
            'recent_win_rate': 0.65,
            'confidence': 0.75,
            'stress': 0.25,
            # ... all ~40 state features
        },
        
        # Action (RL decision)
        'action': 'ENTER_FULL',  # or ENTER_REDUCED, SKIP, WAIT
        
